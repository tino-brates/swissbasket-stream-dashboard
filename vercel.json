{
  "version": 2,
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/(styles\\.css)$", "dest": "/styles.css" },
    { "src": "^/(app\\.js)$", "dest": "/app.js" },
    { "src": "^/(favicon\\.ico)$", "dest": "/favicon.ico" },
    { "src": ".*", "dest": "/api/render" }
  ]
}